{
  "name": "gatsby-source-api-example-basic",
  "description": "Gatsby based site containing a basic confirguration example for the gatsby-source-api plugin",
  "author": "SingleStore Web Team <web@singlestore.com>",
  "scripts": {
    "clean": "npx gatsby clean",
    "build": "npm run build:plugin && npx gatsby build",
    "build:plugin": "cd ../../ && npm run build",
    "prepare": "npm run build:plugin",
    "develop": "npm run build:plugin && npx gatsby develop --port 4000",
    "serve": "npx gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/memsql/gatsby-source-api",
    "directory": "examples/basic"
  },
  "dependencies": {
    "gatsby": "^4.2.0",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "@types/react-helmet": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gatsby-plugin-manifest": "^4.2.0",
    "gatsby-plugin-react-helmet": "^5.2.0"
  }
}
